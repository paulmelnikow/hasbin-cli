{
  "name": "hasbin-cli",
  "version": "1.0.1",
  "description": "From the command line, check whether a binary exists in the PATH environment variable",
  "main": "index.js",
  "bin": {
    "hasbin": "cli.js"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier --write \"**/*.@(js|md|yml)\"",
    "prettier:check": "prettier --check \"**/*.@(js|md|yml)\""
  },
  "files": [
    "*.js"
  ],
  "repository": "paulmelnikow/hasbin-cli",
  "keywords": [
    "bin",
    "check",
    "path",
    "cli"
  ],
  "author": "Paul Melnikow",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "hasbin": "^1.2.3",
    "minimist": "^1.2.0"
  }
}
